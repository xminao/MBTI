syntax = "v1"

info (
	title: "许敏浩"
	desc: "学校管理"
	author: "xminao"
	email: "xminao@yeah.net"
)

type (
	AddCollegeReq {
		CollegeId   int64  `json:"college_id"`
		CollegeName string `json:"college_name"`
	}

	AddCollegeResp {
		Msg string `json:"msg"`
	}
)

service university-api {
	@handler AddCollege
	post /university/getstudent(AddCollegeReq) returns(AddCollegeResp)
}